{
	"name": "obsidian-numbat-plugin",
	"version": "0.1.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"wasm": "wasm-pack build --target=web ./kernels/numbat-kernel --release",
		"dev": "NODE_ENV=development vite build --mode development --watch",
		"build": "tsc -noEmit -skipLibCheck && vite build --mode production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"type": "module",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^18.0.0",
		"builtin-modules": "3.3.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@tailwindcss/postcss": "^4.1.7",
		"tailwindcss": "^4.1.7",
		"vite": "^6.3.5",
		"vite-plugin-wasm-pack": "^0.1.12",
		"wasm-pack": "^0.13.1"
	}
}
